"""https://github.com/coreutils/gnulib/blob/635dbdcf501d52d2e42daf6b44261af9ce2dfe38/m4/memcpy.m4"""

load("//autoconf:autoconf.bzl", "autoconf")
load("//autoconf:checks.bzl", "checks")

autoconf(
    name = "memcpy",
    checks = [
        # Equivalent to gl_FUNC_MEMCPY macro from memcpy.m4
        checks.AC_CHECK_FUNC(
            "memcpy",
            define = "HAVE_MEMCPY",
        ),
    ],
    visibility = ["//visibility:public"],
)
