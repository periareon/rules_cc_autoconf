"""https://github.com/coreutils/gnulib/blob/635dbdcf501d52d2e42daf6b44261af9ce2dfe38/m4/memmove.m4"""

load("//autoconf:autoconf.bzl", "autoconf")
load("//autoconf:macros.bzl", "macros")

autoconf(
    name = "memmove",
    checks = [
        # Check for memmove function
        # Equivalent to gl_FUNC_MEMMOVE macro from memmove.m4
        macros.AC_CHECK_FUNC(
            "memmove",
            define = "HAVE_MEMMOVE",
        ),
    ],
    visibility = ["//visibility:public"],
)
